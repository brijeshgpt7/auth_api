<p id="notice"><%= notice %></p>

<h1>Listing Short Visits</h1>

<table>
  <thead>
    <tr>
      <th>Short url</th>
      <th>Visitor ip</th>
      <th>Visitor city</th>
      <th>Visitior state</th>
      <th>Visitor country iso</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @short_visits.each do |short_visit| %>
      <tr>
        <td><%= short_visit.short_url_id %></td>
        <td><%= short_visit.visitor_ip %></td>
        <td><%= short_visit.visitor_city %></td>
        <td><%= short_visit.visitior_state %></td>
        <td><%= short_visit.visitor_country_iso %></td>
        <td><%= short_visit.latitude %></td>
        <td><%= short_visit.longitude %></td>
        <td><%= link_to 'Show', short_visit %></td>
        <td><%= link_to 'Edit', edit_short_visit_path(short_visit) %></td>
        <td><%= link_to 'Destroy', short_visit, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Short visit', new_short_visit_path %>
